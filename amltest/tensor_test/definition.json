{
	"configuration": {
        "script": "project/tf_mnist_train.py",
        "arguments": [
            "--minibatch_size",
            "64",
            "--learning_rate",
            "0.001",
            "--keep_probability",
			"0.5",
            "--output_dir",
            "outputs",
            "--num_iterations",
            "1000"],
        "framework": "Python",
        "communicator": "None",
        "target": "sparkml-test",
        "maxRunDurationSeconds": null,
        "nodeCount": 1,
        "environment": {
            "python": {
                "interpreterPath": "python",
                "userManagedDependencies": true,
                "condaDependencies": {
                    "name": "venv",
                    "dependencies": ["python=3.6.2", {
                            "pip": ["azureml-defaults"]
                        }]
                }
            },
            "docker": {
                "enabled": true,
                "baseImage": "tensorflow/tensorflow:1.13.2"    
            }
        },
        "history": {
            "outputCollection": true,
        }
    },
    "runtype": "experiment"
}